<h1>Weeks for <%= @league.description %></h1>

<table>
  <thead>
  <tr>
    <th>Week</th>
    <th>Cycle</th>
    <th/>
  </tr>
  </thead>

  <tbody>
  <% @weeks.each do |week| %>
      <tr>
        <td><%= week.week %></td>
        <td><%= week.cycle %></td>
        <td><%= link_to 'Show', week_path(week) %></td>
      </tr>
  <% end %>
  </tbody>
</table>

<%= link_to 'Back', league_path(@league) %>
